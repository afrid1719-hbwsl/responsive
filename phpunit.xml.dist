<?xml version="1.0"?>
<phpunit
	bootstrap=".test/bootstrap.php"
	backupGlobals="false"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
	>
	<testsuites>
		<testsuite>
			<directory prefix="test-" suffix=".php">./.test/core/</directory>
		</testsuite>
	</testsuites>
</phpunit>
